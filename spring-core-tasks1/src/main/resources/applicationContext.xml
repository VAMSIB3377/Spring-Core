<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd">


	<bean id = "Product1" class ="com.vamsi.spring.core.Product">
		<constructor-arg name ="productId" value="A-123"/>
		<constructor-arg name ="productName" value="Laptop"/>
		<constructor-arg name ="productDescription" value="Some Laptop"/>
	</bean>
	
	<bean id = "Product2" class ="com.vamsi.spring.core.Product">
		<constructor-arg name ="productId" value="B-123"/>
		<constructor-arg name ="productName" value="Laptop"/>
		<constructor-arg name ="productDescription" value="Some Laptop"/>
	</bean>

</beans>
